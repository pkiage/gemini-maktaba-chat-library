{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.0.1", 
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "icons": {
    "16": "icon_png/icon16.png",
    "48": "icon_png/icon48.png",
    "128": "icon_png/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://gemini.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gemini.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": {
      "16": "icon_png/icon16.png",
      "32": "icon_png/icon32.png",
      "48": "icon_png/icon48.png"
    },
    "default_title": "__MSG_extName__"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon_png/icon48.png"],
      "matches": ["https://gemini.google.com/*"]
    }
  ]
}